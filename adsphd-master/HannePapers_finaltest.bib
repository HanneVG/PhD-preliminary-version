  urldate = {2013-10-16},
  journal = {Agricultural Water Management},
  author = {Timsina, J. and Godwin, D. and Humphreys, E. and {Yadvinder-Singh} and {Bijay-Singh} and Kukal, S. S. and Smith, D.},
  month = sep,
  year = {2008},
  keywords = {CERES-Wheat,Crop and irrigation water productivity,Irrigation scheduling,Model application,Model calibration,Model evaluation,Triticum aestivum},
  pages = {1099--1110},
  file = {ScienceDirect Full Text PDF:C\:\\DATA_HanneV\\Literatuur\\storage\\25NRPM5E\\Timsina et al. - 2008 - Evaluation of options for increasing yield and wat.pdf:application/pdf}
