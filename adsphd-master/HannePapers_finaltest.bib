@incollection{mitchell1997,
  series = {Systems Approaches for Sustainable Agricultural Development},
  title = {Comparison of predictions and observations to assess model performance: a method of empirical validation},
  copyright = {\textcopyright{}1997 Springer Science+Business Media Dordrecht},
  isbn = {978-90-481-4763-2 978-94-017-0754-1},
  shorttitle = {Comparison of predictions and observations to assess model performance},
  abstract = {Empirical validation is the comparison of model predictions with observations from the real system, together with an assessment of whether the model is adequate for its purpose. When the data available consist of pairs of predictions and observations covering the range of model operation, they are often presented as a scatter graph, sometimes with a regression line added. There are several reasons why regression is not appropriate for this task. An alternative method described uses the deviations (prediction minus observation) plotted against the observations. From this graph of deviations the bias and precision of the model can be assessed, and how they vary along the range of operation. The two criteria for adequacy of the model are the envelope of acceptable precision and the proportion of points that must lie within it. The envelope of acceptable precision is defined with reference to the purpose of the model, most commonly by comparison with the precision of the observations. The proportion of points in the envelope can be set at 0.95 by analogy with confidence limits. This method is demonstrated on two examples from different models, and it can be extended to other forms of validation data. Although statistical tests are not involved, which avoids the problem of satisfying assumptions, this method is simple, objective and quantitative and can be made rigorous by definition of the criteria in advance.},
  language = {en},
  timestamp = {2014-12-08T14:34:37Z},
  number = {6},
  urldate = {2014-12-08},
  booktitle = {Applications of {{Systems Approaches}} at the {{Field Level}}},
  publisher = {{Springer Netherlands}},
  author = {Mitchell, P. L. and Sheehy, J. E.},
  editor = {Kropff, M. J. and Teng, P. S. and Aggarwal, P. K. and Bouma, J. and Bouman, B. A. M. and Jones, J. W. and {van Laar}, H. H.},
  month = jan,
  year = {1997},
  keywords = {Agriculture,Ecology,Economic Growth,Model,modelling,precision,Regression,Validation},
  pages = {437--451}
}

