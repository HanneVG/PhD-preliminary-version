  keywords = {Asia,Crop yield,food security,Ozone,Rice,Soybean,Wheat},
  pages = {1945--1953},
  file = {ScienceDirect Full Text PDF:C\:\\DATA_HanneV\\Literatuur\\storage\\MHQKZXHD\\Emberson et al. - 2009 - A comparison of North American and Asian exposure–.pdf:application/pdf}
