  keywords = {Agricultural pollution,California,Climate change,Pesticides,SWAT,Watershed modeling},
  pages = {223--234},
  file = {ScienceDirect Full Text PDF:C\:\\DATA_HanneV\\Literatuur\\storage\\Z5F69MCH\\Ficklin et al. - 2010 - Sensitivity of agricultural runoff loads to rising.pdf:application/pdf}
}

