  keywords = {Agricultural modeling,ClimGen,Cropping systems,CropSyst,Farming systems,Simulation models,Watershed simulation},
  pages = {289--307},
  file = {ScienceDirect Full Text PDF:C\:\\DATA_HanneV\\Literatuur\\storage\\Q467B8IX\\Stöckle et al. - 2003 - CropSyst, a cropping systems simulation model.pdf:application/pdf}
}

