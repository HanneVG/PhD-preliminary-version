  keywords = {AquaCrop,Teff,Validation,water use efficiency},
  pages = {196--204},
  file = {ScienceDirect Full Text PDF:C\:\\DATA_HanneV\\Literatuur\\storage\\HIGEGHQX\\Araya e.a. - 2010 - Simulating yield response to water of Teff (Eragro.pdf:application/pdf}
