}

@article{muluneh2014,
  title = {Impact of predicted changes in rainfall and atmospheric carbon dioxide on maize and wheat yields in the {{Central Rift Valley}} of {{Ethiopia}}},
  issn = {1436-3798, 1436-378X},
  doi = {10.1007/s10113-014-0685-x},
  abstract = {This study assesses potential impacts of climate change on maize and wheat yields in the Central Rift Valley (CRV) of Ethiopia. We considered effects of elevated atmospheric carbon dioxide (CO2) and changes in rainfall during the main (Kiremt) and the short (Belg) rainfall cropping seasons during the two future periods (2020\textendash{}2049 and 2066\textendash{}2095). The MarkSimGCM daily weather generator was used to generate projected rainfall and temperature data using the outputs from ECHAM5 general circulation model and ensemble mean of six models under A2 (high) and B1 (low) emission scenarios. Crop yield simulations were made with the FAO's AquaCrop model. The projected rainfall during Kiremt increases by 12\textendash{}69 \% while rainfall during Belg decreases by 20\textendash{}68 \%. The combined effect of elevated CO2 and projected climate factors increases maize yield by up to 59 \% in sub-humid/humid areas of the CRV, but could result in a decrease of up to 46 \% in the semiarid areas under ECHAM5 model. However, the maize yield increases in all parts of the CRV under the ensemble mean of models. Wheat yield shows no significant response to the projected rainfall changes, but increases by up to 40 \% due to elevated CO2. Our results generally suggest that climate change will increase crop yields in the sub-humid/humid regions of the CRV. However, in the semi-arid parts the overall projected climate change will affect crop yields negatively.},
  language = {en},
  timestamp = {2016-05-14T16:18:15Z},
  urldate = {2014-11-27},
  journal = {Reg Environ Change},
  author = {Muluneh, Alemayehu and Biazin, Birhanu and Stroosnijder, Leo and Bewket, Woldeamlak and Keesstra, Saskia},
  month = sep,
  year = {2014},
  keywords = {Belg,Climate change,Climate Change/Climate Change Impacts,Crop yield,Elevated CO2,Geography (general),Kiremt,Nature Conservation,Oceanography,rainfall,Regional/Spatial Science},
  pages = {1--15}
}

@article{shrestha2014d,
  title = {Assessment of the climate-change impacts and evaluation of adaptation measures for paddy productivity in {{Quang Nam}} province, {{Vietnam}}},
  issn = {1611-2490, 1611-2504},
  doi = {10.1007/s10333-014-0434-2},
  abstract = {This study investigates the impacts of climate change on winter and summer paddy yields and evaluates adaptation measures in order to overcome the projected impacts of climate change in Quang Nam province of Vietnam. Future climate-change scenarios for the periods of 2020s, 2050s and 2080s were projected by downscaling the outputs of HadCM3 general circulation model for the two selected IPCC SRES A2 and B2 scenarios. The AquaCrop model was used to simulate the impact of future climate on paddy yield. The minimum and maximum temperatures of the basin are expected to increase by 0.35\textendash{}1.72 and 0.93\textendash{}3.69 \ensuremath{^\circ}C, respectively in future. Similarly, the annual precipitation is expected to increase by 9.75 \% in future. Results show that climate change will reduce paddy yield in winter season for both scenarios and all time periods, whereas an increase in the yield is expected in the summer season for 2020s and 2050s relative to baseline yield. The overall decrease in paddy yield in winter season can be offset, and paddy yield of summer season can be enhanced to potential level by altering the transplanting dates and by introducing supplementary irrigation. Late transplanting of paddy shows an increase in yield by 20\textendash{}27 \% in future, whereas supplementary irrigation in winter season paddy shows an increase in yield up to 42 \% in future. The results of this study can be used by policy makers and key stakeholders to optimize paddy production under changing climate regimes in Quang Nam province of Vietnam.},
  language = {en},
  timestamp = {2016-05-14T16:48:40Z},
  urldate = {2014-11-27},
  journal = {Paddy Water Environ},
  author = {Shrestha, Sangam and Trang, Bui Thi Thu},
  month = apr,
  year = {2014},
  keywords = {Agriculture,Agro-adaptation,AquaCrop,Climate change,Ecotoxicology,Geoecology/Natural Processes,Hydrogeology,Paddy,Soil Science \& Conservation,Vietnam,Water Quality/Water Pollution},
  pages = {1--13}
}

