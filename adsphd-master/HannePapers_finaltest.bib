  keywords = {crop cycle lengths,el nino,maize,Micro-catchment,model combination,plant density strategies,PUTU crop model,rainfall intensity model,rainfall-runoff model,Rainwater harvesting,Risk analysis,Runoff agriculture,Southern oscillation index,sowing strategies},
  pages = {299--311},
  file = {1-s2.0-S0140196307001231-main.pdf:C\:\\DATA_HanneV\\Literatuur\\storage\\7727QMUN\\1-s2.0-S0140196307001231-main.pdf:application/pdf}
