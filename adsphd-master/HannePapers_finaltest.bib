@article{ni2000,
  title = {Oryza sativa plant traits conferring competitive ability against weeds},
  volume = {48},
  issn = {0043-1745},
  doi = {10.1614/0043-1745(2000)048[0200:OSPTCC]2.0.CO;2},
  abstract = {Abstract This study was conducted in lowland fields at the International Rice Research Institute in the 1994 wet season and the 1995 dry season to determine Oryza sativa plant traits that confer competitive ability against weeds when pregerminated seeds are sown on puddled soil. Initial biomass (IB), crop growth rate (CGR), leaf area index (LAI), and biomass at tillering of O. sativa plants were associated with their competitiveness against weeds, whereas relative growth rate, net assimilation rate, and tillering capacity of O. sativa were not. Biomass at tillering affected weed biomass directly, and IB, CGR, and LAI indirectly affected weed biomass through O. sativa biomass. Biomass at tillering was the best predictor of modern cultivar competitiveness against weeds. Nomenclature: Oryza sativa L., rice.},
