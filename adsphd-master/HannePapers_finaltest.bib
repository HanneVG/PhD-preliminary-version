@article{iqbal2014,
  title = {Evaluation of the {{FAO AquaCrop}} model for winter wheat on the {{North China Plain}} under deficit irrigation from field experiment to regional yield simulation},
  volume = {135},
  issn = {0378-3774},
  doi = {10.1016/j.agwat.2013.12.012},
  abstract = {Winter wheat is the main crop on the North China Plain (NCP), and in this region the most limiting factor for the crop is water. The objective of this study was to adapt and test the ability of the FAO-developed AquaCrop model (v3.1) to simulate winter wheat grain yield, biomass, actual evapotranspiration (ETa) and total soil water content (0\textendash{}120 cm). Field experiments were conducted under deficit irrigation at the Luancheng Agro-ecosystem station (NCP) in 1998\textendash{}2001, and the AquaCrop model was calibrated with treatment D (1999\textendash{}2000); the rest of the data was used for validation of the model. The AquaCrop model was revalidated with data on measured grain yield from the experimental station for 1990\textendash{}2010, considering actual field conditions. The second revalidation was done with the statistical grain yield for 1995\textendash{}2010 in the study region. For the model validation, the significant differences between simulated and observed grain yield, biomass and ETa were in the order of: rainfed treatment \ensuremath{>} well-watered treatment \ensuremath{>} moderate water stress. Total soil water simulated by AquaCrop tends to follow closely the trend in the measured data, but with slight underestimations for irrigated treatments and significant overestimations for rainfed treatments. In general, errors in the model's evaluation such as RMSE and Willmot's d statistics were for grain yield (0.58 Mg ha-1, 0.92), biomass (0.87 Mg ha-1, 0.95), ETa (33.2 mm, 0.93) and soil water content (24.5\textendash{}37.6 mm, 0.85\textendash{}0.90). The overall results based on extensive validation and revalidation showed that AquaCrop is a valid model and can be used with a reliable degree of accuracy for optimizing winter wheat grain yield production and water requirement on the NCP.},
  timestamp = {2014-03-27T15:24:54Z},
  urldate = {2014-03-27},
  journal = {Agricultural Water Management},
  author = {Iqbal, M. Anjum and Shen, Yanjun and Stricevic, Ruzica and Pei, Hongwei and Sun, Hongyoung and Amiri, Ebrahim and Penas, Angel and {del Rio}, Sara},
  month = mar,
  year = {2014},
  keywords = {AquaCrop,Deficit irrigation,North China Plain,Soft-copy*,Winter wheat},
  pages = {61--72},
  file = {ScienceDirect Full Text PDF:C\:\\DATA_HanneV\\Literatuur\\storage\\5HKCRAG8\\Iqbal e.a. - 2014 - Evaluation of the FAO AquaCrop model for winter wh.pdf:application/pdf}
}

